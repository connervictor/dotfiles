AddKeysToAgent yes
UseKeychain yes
User git
IdentityFile ~/.ssh/creativecreature
IdentitiesOnly yes

Host localhost
HostName 127.0.0.1
Port 22
User pair
IdentityFile ~/.ssh/private_rsa
